--#	define EMPTY_FUNC_DEF(x) static inline void x(void) {}
--	EMPTY_FUNC_DEF(init_extensions)
--	EMPTY_FUNC_DEF(init_extensions4)
--	EMPTY_FUNC_DEF(init_extensions6)
--	EMPTY_FUNC_DEF(init_extensionsa)
