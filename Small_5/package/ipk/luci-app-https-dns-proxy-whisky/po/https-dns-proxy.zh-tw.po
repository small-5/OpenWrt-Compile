msgid "HTTPS DNS Proxy is a light-weight DNS<-->HTTPS, non-caching translation proxy for the RFC 8484 DNS-over-HTTPS standard. It receives regular (UDP) DNS requests and issues them via DoH"
msgstr "HTTPS DNS Proxy是一個輕量化的基於RFC8484標準的非緩存型服務代理，接收基於UDP的常規DNS請求並通過HTTPS向上遊DoH伺服器遞歸查詢"

msgid "HTTPS DNS Proxy Settings"
msgstr "HTTPS DNS Proxy 設定"

msgid "Enable"
msgstr "啟用"

msgid "Resolver URL"
msgstr "DoH伺服器鏈接"

msgid "For example https://dns.google/dns-query"
msgstr "例如 https://dns.google/dns-query"

msgid "DNS Server"
msgstr "DNS伺服器"

msgid "DNS Server for resolving DoH domain names,The format is addr:port.Use commas to separate<br/>When specifying a port for IPv6,enclose the address in []<br/>For example 8.8.8.8,8.8.4.4:53,2001:4860:4860::8888,[2001:4860:4860::8844]:53"
msgstr "解析DoH域名的DNS伺服器,格式為 addr:port.使用英文逗號分割<br/>使用IPv6指定通訊埠時,需要將IP放入[]內<br/>例如8.8.8.8,8.8.4.4:53,2001:4860:4860::8888,[2001:4860:4860::8844]:53"

msgid "Ali"
msgstr "阿里"

msgid "Google"
msgstr "谷歌"

msgid "Local IPv4/v6 address to bind to (Default 127.0.0.1)"
msgstr "本地IPv4/v6監聽位址(默認127.0.0.1)"

msgid "Local port to bind to (Default 5053)"
msgstr "本地通訊埠(默認5053)"

msgid "Redirect"
msgstr "重定向"

msgid "Dnsmasq upstream server"
msgstr "Dnsmasq上遊伺服器"

msgid "Run as Dnsmasq upstream server"
msgstr "作為Dnsmasq上遊伺服器"

msgid "Use IPv4 communication"
msgstr "使用IPv4通訊"

msgid "Connect to DoH server using IPv4"
msgstr "使用IPv4連接到DoH伺服器"

msgid "Proxy server"
msgstr "代理伺服器"

msgid "Proxy server(optional)<br/>Supported Protocols: http, https, socks4a, socks5h<br/>For example: socks5://127.0.0.1:1080, http://127.0.0.1:8080"
msgstr "代理伺服器(可選)<br/>支持的協議: http, https, socks4a, socks5h<br/>例如: socks5://127.0.0.1:1080, http://127.0.0.1:8080"
